var a: Int = read_int();
while a != 0 do {
	print_int(a);
	a = read_int();
}

input 5
prints 5
input 1
prints 1
input 100
prints 100
input 0

---

if true then {
	true
} else {
	false
}

prints true

---

if false then {
	true
} else {
	false
}

prints false

---

1 == 1

prints true

---

1 != 1

prints false

---

true == true

prints true

---

false != false

prints false
